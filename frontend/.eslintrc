{
  "extends": "airbnb",
  "rules": {
    "consistent-return": [1],
    "strict": [0],
    "max-len": ["warn", 200],
    "no-underscore-dangle": [1],
    "no-console": "off",
    "react/jsx-props-no-spreading": "off",
    //shloch
    "jsx-a11y/label-has-associated-control": ["error", {
      "required": {
        "some": ["nesting", "id"]
      }
    }],
    "jsx-a11y/label-has-for": ["error", {
      "required": {
        "some": ["nesting", "id"]
      }
    }]
  },
  "env": {
    "browser": true
  }
 
   

}
